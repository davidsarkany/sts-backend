{
  "name": "sts-backend",
  "version": "1.1.0",
  "description": "sts-backend",
  "main": "npm run serve",
  "scripts": {
    "build": "tsc",
    "test": "jest --forceExit --coverage --verbose",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidsarkany/sts-backend.git"
  },
  "author": "David Sarkany",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidsarkany/sts-backend/issues"
  },
  "homepage": "https://github.com/davidsarkany/sts-backend#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.7.0",
    "fastify-formbody": "^5.0.0",
    "fastify-rate-limit": "^4.0.3"
  }
}
